<html lang="en">
<head>
  <meta charset="utf-8" />
  #// 2
  <title>#(title) | Hello World</title>
</head>
<body>
  #// 3
  <h1>Acronyms</h1>
  #if(acronyms) {
    <table>
        <thead>
            <tr>
                <th>Short</th>
                <th>Long</th>
            </tr>
        </thead>
        <tbody>
        #for(acronym in acronyms) {
            <tr>
                <td>#(acronym.short)</td>
                <td>#(acronym.long)</td>
            </tr>
        }
        </tbody>
    </table>
  } else {
    <h2> There arenâ€™t any acronyms yet! </h2>
  }

</body>
</html>
